const la1 = document.getElementById('lab1');
const la2 = document.getElementById('lab2');
const la3 = document.getElementById('lab3');

let button1;
let button2;
let button3;
const btn = document.getElementById('but');
btn.onclick = function(){
    button1 = Math.floor(Math.random()*6)+1;
    button2 = Math.floor(Math.random()*6)+1;
    button3 = Math.floor(Math.random()*6)+1;
    la1.textContent=button1;
    la2.textContent=button2;
    la3.textContent=button3;
}
